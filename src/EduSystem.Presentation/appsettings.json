{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=edu_system.db;Cache=Shared"
  },
  "Emails": {
    "Smtp": {
      "Enabled": false,
      "Host": "{{mail_host}}",
      "Port": 26,
      "EnableSsl": false,
      "SenderEmail": "{{sender_email}}",
      "SenderPassword": "{{sender_password}}"
    },
    "SendGrid": {
      "Enabled": false,
      "ApiKey": "{{api_key}}",
      "Email": "{{email}}",
      "Name": "{{name}}"
    }
  },
  "ReqResToken": "SomeVerySecretToken",
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
